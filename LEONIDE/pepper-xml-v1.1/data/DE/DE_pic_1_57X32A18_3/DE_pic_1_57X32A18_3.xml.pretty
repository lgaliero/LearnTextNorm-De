    <tran_emphasis_id11 class="medium-tran_emphasis_id11" tagcode="1541468933171">
     <tran_capitalisation_id85 class="medium-tran_capitalisation_id85" tagcode="1611824555421" tran_capitalisation_target="Traum" tran_capitalisation_type="01 completely capitalised">
      TRAUM
     </tran_capitalisation_id85>
