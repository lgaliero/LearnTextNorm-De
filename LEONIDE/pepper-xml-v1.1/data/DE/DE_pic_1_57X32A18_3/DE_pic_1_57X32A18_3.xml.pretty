    <tran_emphasis_id11 class="medium-tran_emphasis_id11" tagcode="1541468929594">
     <tran_capitalisation_id85 class="medium-tran_capitalisation_id85" tagcode="1611824547809" tran_capitalisation_target="Schule" tran_capitalisation_type="01 completely capitalised">
      SCHULE
     </tran_capitalisation_id85>
    </tran_emphasis_id11>
