    <orth_error_id12 class="medium-orth_error_id12" orth_error_target="verändert" tagcode="1551017791729">
     <tran_ambiguous_id3 class="medium-tran_ambiguous_id3" tagcode="1551017809903" tran_alternative_1="verandert" tran_alternative_2="verändert">
      verandert
     </tran_ambiguous_id3>
