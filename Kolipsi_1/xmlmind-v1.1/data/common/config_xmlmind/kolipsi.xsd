			<!-- Annotation -->
			<xs:element minOccurs="0" maxOccurs="1" ref="hyphen" />
			<!-- Annotation -->
			<xs:element minOccurs="0" maxOccurs="1" ref="image" />
			<!-- Markup -->
			<xs:element minOccurs="0" maxOccurs="1" ref="over" />
			<!-- Markup -->
			<xs:element minOccurs="0" maxOccurs="1" ref="overwrite" />
			<!-- Markup -->
			<xs:element minOccurs="0" maxOccurs="1" ref="palimpsest" />
			<!-- Structural -->
			<xs:element minOccurs="0" maxOccurs="1" ref="par" />
			<!-- Annotation -->
			<xs:element minOccurs="0" maxOccurs="1" ref="reduction" />
			<!-- Annotation -->
			<xs:element minOccurs="0" maxOccurs="1" ref="sic" />
			<!-- Markup -->
			<xs:element minOccurs="0" maxOccurs="1" ref="strikeover" />
			<!-- Markup -->
			<xs:element minOccurs="0" maxOccurs="1" ref="symbol" />
