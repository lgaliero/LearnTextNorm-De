			<xs:enumeration value="14 omissions: of one grapheme" />
			<xs:enumeration value="15 insertions: of double consonants" />
			<xs:enumeration value="16 insertions: of a vowel: (ie) instead of (i)" />
			<xs:enumeration value="17 insertions: other cases" />
			<xs:enumeration value="18 transpositions: of vowels" />
			<xs:enumeration value="19 transpositions: of  (e/ä) / (ä/e)" />
			<xs:enumeration value="20 transpositions: of  (i/e) / (e/i)" />
			<xs:enumeration value="21 transpositions: of vowel and umlaut" />
			<xs:enumeration value="22 transpositions: of consonants: Fortis/Lenis" />
			<xs:enumeration value="23 transpositions: of consonants: other cases" />
			<xs:enumeration value="24 transpositions: of (ss) and (ß)" />
			<xs:enumeration value="25 incorrect positioning of two graphemes" />
			<xs:enumeration value="26 missing or incorrect use of apostrophes" />
