			<xs:enumeration value="08 tog instead of sep: minimal phraseologism" />
			<xs:enumeration value="09 tog instead of sep: other cases" />
			<xs:enumeration value="10 missing or unnecessary hyphen" />
			<xs:enumeration value="11 omissions: of double consonants" />
			<xs:enumeration value="12 omissions: of a vowel: (i) instead of (ie)" />
