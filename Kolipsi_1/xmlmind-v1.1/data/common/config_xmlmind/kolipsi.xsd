			<!-- Functional -->
			<xs:element minOccurs="0" maxOccurs="1" ref="direct_speech" />
			<!-- Markup -->
			<xs:element minOccurs="0" maxOccurs="1" ref="emoticon" />
			<!-- Markup -->
